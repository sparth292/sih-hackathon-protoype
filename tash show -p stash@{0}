[32m+[m[32m  Future<void> _initFlow() async {[m
[32m+[m[32m    await _ensureLocationPermission();[m
[32m+[m[32m    await _loadMurtiMarkerIcon();[m
[32m+[m[32m    await _loadUserPosition();[m
